    <moduleMetadata>
      <moduleType>topicshell</moduleType>
      <moduleShortName>iirds_reference</moduleShortName>
      <shellPublicIds>
        <!--        <dtdShell>-//IIRDS//DTD DITA<var presep=" " name="ditaver"/> Reference//EN</dtdShell>-->
        <!--        <rncShell>urn:dita:iirds:rnc:iirds_reference.rnc<var presep=":" name="ditaver"/></rncShell>-->
