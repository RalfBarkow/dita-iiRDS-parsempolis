    <a:documentation>ELEMENT TYPE DECLARATIONS</a:documentation>
    <div>
      <a:documentation> LONG NAME: IIRDS Metadata Element</a:documentation>
      <define name="iirds-metadata.element">
        <element name="iirds-metadata" dita:longName="iiRDS Metadata Elements">
          <ref name="iirds-metadata.content"/>
          <ref name="iirds-metadata.attlist"/>
        </element>
      </define>
      <define name="iirds-metadata.content">
        <zeroOrMore>
          <choice>
            <ref name="event.element"/>
            <ref name="qualification.element"/>
            <ref name="identity.element"/>
            <ref name="content-lifecycle-status.element"/>
            <ref name="party.element"/>
          </choice>
          <!--<choice>
            <ref name="document-type.element"/>
            <ref name="information-subject.element"/>
          </choice>-->
        </zeroOrMore>
      </define>
      <define name="iirds-metadata.attlist" combine="interleave">
        <ref name="iirds-metadata.attributes"/>
      </define>
      <define name="iirds-metadata.attributes">
        <ref name="univ-atts"/>
        <optional>
          <attribute name="class"
            a:defaultValue="+ topic/data iirds-d/iirds-data iirds-d/iirds-adminmeta "/>
        </optional>
<!--        ACHTUNG: Class-Attribute vorÃ¼bergehend auf adminmeta gesetzt, weil das andere Plugin noch nicht nachgezogen wurde-->
      </define>
    </div>
    
