    <div>
      <a:documentation>Metadata elements within Functional Metadata</a:documentation>
      <div>
        <a:documentation> LONG NAME: IIRDS Event Subclass</a:documentation>
        <define name="event.element">
          <element name="event" dita:longName="iiRDS Event Subclass">
            <ref name="event.content"/>
            <ref name="event.attlist"/>
          </element>
        </define>
        <define name="event.content">
               <a:documentation>Each event should have event type and event code. As a start, both are optional, to make it easier for testing
               </a:documentation>     <optional>
            <ref name="event-code.element"/>
          </optional>
          <optional>
            <ref name="event-type.element"/>
          </optional>
        </define>
        <define name="event.attlist" combine="interleave">
          <ref name="event.attributes"/>
        </define>
        <define name="event.attributes">
          <ref name="univ-atts"/>
          <optional>
            <attribute name="class" a:defaultValue="+ topic/data iirds-d/iirds-data iirds-d/event "
            />
          </optional>
        </define>
      </div>
      <div>
        <a:documentation> LONG NAME: IIRDS Event Code Element</a:documentation>
        <define name="event-code.element">
          <element name="event-code" dita:longName="iiRDS Event Code Element">
            <ref name="event-code.content"/>
            <ref name="event-code.attlist"/>
          </element>
        </define>
        <define name="event-code.content">
          <ref name="iirds-metadata.cnt"/>
        </define>
        <define name="event-code.attlist" combine="interleave">
          <ref name="event-code.attributes"/>
        </define>
        <define name="event-code.attributes">
          <ref name="univ-atts"/>
          <optional>
            <attribute name="class"
              a:defaultValue="+ topic/data iirds-d/iirds-data iirds-d/event iirds-d/event-code "/>
          </optional>
        </define>
      </div>
      <div>
        <a:documentation> LONG NAME: IIRDS Event Type Element</a:documentation>
        <define name="event-type.element">
          <element name="event-type" dita:longName="iiRDS Event Type Element">
            <ref name="event-type.content"/>
            <ref name="event-type.attlist"/>
          </element>
        </define>
        <define name="event-type.content">
          <ref name="iirds-metadata.cnt"/>
        </define>
        <define name="event-type.attlist" combine="interleave">
          <ref name="event-type.attributes"/>
        </define>
        <define name="event-type.attributes">
          <ref name="univ-atts"/>
          <optional>
            <attribute name="class"
              a:defaultValue="+ topic/data iirds-d/iirds-data iirds-d/event iirds-d/event-type "/>
          </optional>
        </define>
      </div>
