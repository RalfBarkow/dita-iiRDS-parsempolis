    <div>
      <a:documentation>Metadata elements within Administrative Metadata </a:documentation>
      <div>
        <a:documentation> LONG NAME: iiRDS Identity Element</a:documentation>
        <define name="identity.element">
          <element name="identity" dita:longName="iiRDS Identity">
            <ref name="identity.content"/>
            <ref name="identity.attlist"/>
          </element>
        </define>
        <define name="identity.content">
          <a:documentation>Each identity must have an identifier and an identity domain. Party is optional.</a:documentation>
          <interleave>
            <ref name="identity-identifier.element"/>
            <ref name="identity-domain.element"/>
            <optional>
              <ref name="party.element"/>
            </optional>
          </interleave>  
        </define>
        <define name="identity.attlist" combine="interleave">
          <ref name="identity.attributes"/>
        </define>
        <define name="identity.attributes">
          <ref name="univ-atts"/>
          <optional>
            <attribute name="class"
              a:defaultValue="+ topic/data iirds-d/iirds-data iirds-d/identity "/>
          </optional>
        </define>
      </div>
      <div>
        <a:documentation> LONG NAME: iiRDS Identity Identifier Element</a:documentation>
        <define name="identity-identifier.element">
          <element name="identity-identifier" dita:longName="iiRDS Identity Identifier">
            <ref name="identity-identifier.content"/>
            <ref name="identity-identifier.attlist"/>
          </element>
        </define>
        <define name="identity-identifier.content">
          <ref name="iirds-metadata.cnt"/>
        </define>
        <define name="identity-identifier.attlist" combine="interleave">
          <ref name="identity-identifier.attributes"/>
        </define>
        <define name="identity-identifier.attributes">
          <ref name="univ-atts"/>
          <optional>
            <attribute name="class"
              a:defaultValue="+ topic/data iirds-d/iirds-data iirds-d/identity-identifier "/>
          </optional>
        </define>
      </div>
      <div>
        <a:documentation> LONG NAME: iiRDS Identity Domain Element</a:documentation>
        <define name="identity-domain.element">
          <element name="identity-domain" dita:longName="iiRDS Identity Domain">
            <ref name="identity-domain.content"/>
            <ref name="identity-domain.attlist"/>
          </element>
        </define>
        <define name="identity-domain.content">
          <ref name="iirds-metadata.cnt"/>          
        </define>
        <define name="identity-domain.attlist" combine="interleave">
          <ref name="identity-domain.attributes"/>
        </define>
        <define name="identity-domain.attributes">
          <ref name="univ-atts"/>
          <optional>
            <attribute name="class"
              a:defaultValue="+ topic/data iirds-d/iirds-data iirds-d/identity-domain "/>
          </optional>
        </define>
      </div>
      
      <div>
        <a:documentation> LONG NAME: iiRDS Party</a:documentation>
        <define name="party.element">
          <element name="party" dita:longName="iiRDS Party">
            <ref name="party.content"/>
            <ref name="party.attlist"/>
          </element>
        </define>
        <define name="party.content">
          <interleave>                   
            <ref name="party-role.element"/>        
            <choice>
              <ref name="personinfo.element"/>
              <ref name="organizationinfo.element"/>
            </choice> 
          </interleave>
        </define>
        <define name="party.attlist" combine="interleave">
          <ref name="party.attributes"/>
        </define>
        <define name="party.attributes">
          <ref name="univ-atts"/>
          <optional>
            <attribute name="class"
              a:defaultValue="+ topic/data iirds-d/iirds-data iirds-d/party "/>
          </optional>
        </define>
      </div>
      <div>
        <a:documentation> LONG NAME: iiRDS Party Role</a:documentation>
        <define name="party-role.element">
          <element name="party-role" dita:longName="iiRDS Party Role">
            <ref name="party-role.content"/>
            <ref name="party-role.attlist"/>
          </element>
        </define>
        <define name="party-role.content">
          <ref name="iirds-metadata.cnt"/>          
        </define>
        <define name="party-role.attlist" combine="interleave">
          <ref name="party-role.attributes"/>
        </define>
        <define name="party-role.attributes">
          <ref name="univ-atts"/>
          <optional>
            <attribute name="class"
              a:defaultValue="+ topic/data iirds-d/iirds-data iirds-d/party-role "/>
          </optional>
        </define>
      </div>
      
      <div>
        <a:documentation> LONG NAME: iiRDS Content Lifecycle Status</a:documentation>
        <define name="content-lifecycle-status.element">
          <element name="content-lifecycle-status" dita:longName="iiRDS Content Lifecycle Status">
            <ref name="content-lifecycle-status.content"/>
            <ref name="content-lifecycle-status.attlist"/>
          </element>
        </define>
        <define name="content-lifecycle-status.content">
          <ref name="iirds-metadata.cnt"/>
          <interleave>
            <ref name="content-lifecycle-status-value.element"/>            
            <optional>
              <ref name="status-purpose.element"/>
            </optional>
            <optional>
              <ref name="status-comment.element"/>
            </optional>
            <optional>
              <ref name="critdates.element"/>
            </optional>
            <optional>
              <ref name="party.element"/>
            </optional>
          </interleave>
        </define>
        <define name="content-lifecycle-status.attlist" combine="interleave">
          <ref name="content-lifecycle-status.attributes"/>
        </define>
        <define name="content-lifecycle-status.attributes">
          <ref name="univ-atts"/>
          <optional>
            <attribute name="class"
              a:defaultValue="+ topic/data iirds-d/iirds-data iirds-d/content-lifecycle-status "/>
          </optional>
        </define>
      </div>
      <div>
        <a:documentation> LONG NAME: iiRDS Content Lifecycle Status Value</a:documentation>
        <define name="content-lifecycle-status-value.element">
          <element name="content-lifecycle-status-value" dita:longName="iiRDS Content Lifecycle Status Value">
            <ref name="content-lifecycle-status-value.content"/>
            <ref name="content-lifecycle-status-value.attlist"/>
          </element>
        </define>
        <define name="content-lifecycle-status-value.content">
          <ref name="iirds-metadata.cnt"/>          
        </define>
        <define name="content-lifecycle-status-value.attlist" combine="interleave">
          <ref name="content-lifecycle-status-value.attributes"/>
        </define>
        <define name="content-lifecycle-status-value.attributes">
          <ref name="univ-atts"/>
          <optional>
            <attribute name="class"
              a:defaultValue="+ topic/data iirds-d/iirds-data iirds-d/content-lifecycle-status-value "/>
          </optional>
        </define>
      </div>
      <div>
        <a:documentation> LONG NAME: iiRDS Content Lifecycle Status Comment</a:documentation>
        <define name="status-comment.element">
          <element name="status-comment" dita:longName="iiRDS Content Lifecycle Status Comment">
            <ref name="status-comment.content"/>
            <ref name="status-comment.attlist"/>
          </element>
        </define>
        <define name="status-comment.content">
          <ref name="iirds-metadata.cnt"/>          
        </define>
        <define name="status-comment.attlist" combine="interleave">
          <ref name="content-lifecycle-status-value.attributes"/>
        </define>
        <define name="status-comment.attributes">
          <ref name="univ-atts"/>
          <optional>
            <attribute name="class"
              a:defaultValue="+ topic/data iirds-d/iirds-data iirds-d/status-comment "/>
          </optional>
        </define>
      </div>
      <div>
        <a:documentation> LONG NAME: iiRDS Content Lifecycle Status Purpose</a:documentation>
        <define name="status-purpose.element">
          <element name="status-purpose" dita:longName="iiRDS Content Lifecycle Status Purpose">
            <ref name="status-purpose.content"/>
            <ref name="status-purpose.attlist"/>
          </element>
        </define>
        <define name="status-purpose.content">
          <ref name="iirds-metadata.cnt"/>          
        </define>
        <define name="status-purpose.attlist" combine="interleave">
          <ref name="status-purpose.attributes"/>
        </define>
        <define name="status-purpose.attributes">
          <ref name="univ-atts"/>
          <optional>
            <attribute name="class"
              a:defaultValue="+ topic/data iirds-d/iirds-data iirds-d/status-purpose "/>
          </optional>
        </define>
      </div>
    </div>
  </div>
  
  
  <div>
    <a:documentation>Basic Patterns</a:documentation>
    <define name="iirds-metadata.cnt">
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="keyword"/>
          <ref name="ph"/>
          <ref name="draft-comment" dita:since="1.3"/>
        </choice>
      </zeroOrMore>
    </define>
  </div>
